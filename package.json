{
  "name": "jjkim-frameless-titlebar-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "elec/main.js",
  "scripts": {
    "start": "webpack-dev-server --port 4500",
    "build": "webpack --mode=production",
    "dev": "wait-on http://localhost:4500 && cross-env ELECTRON_START_URL=http://localhost:4500/ electron .",
    "pkgs": "npx electron-builder --win --dir",
    "prepkgs": "yarn build",
    "asar-unpack": "npx asar extract pkgs/win-unpacked/resources/app.asar pkgs/asar-unpack/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.0",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "wait-on": "^5.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.2.4",
    "electron-settings": "^4.0.2",
    "electron-updater": "^4.3.4",
    "frameless-titlebar": "^2.1.3",
    "is-electron": "^2.2.0",
    "notistack": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "build": {
    "files": [
      "dist/**/*",
      "elec/**/*",
      "node_modules/**/*"
    ],
    "productName": "frameless-titlebar-usage",
    "appId": "com.jjkim.framelesstitlebar",
    "extends": null,
    "directories": {
      "buildResources": "public",
      "output": "pkgs"
    },
    "target": "zip",
    "publish": [
      {
        "provider": "github",
        "owner": "objects76",
        "repo": "autoupdate1"
      }
    ]
